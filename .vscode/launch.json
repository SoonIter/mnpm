  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'mnpm'",
      "cargo": {
        "args": [
          "build",
          "--bin=mnpm",
          "--package=mnpm"
        ],
        "filter": {
          "name": "mnpm",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}/examples/playground"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in library 'mnpm'",
      "cargo": {
        "args": ["test", "--no-run", "--lib", "--package=mnpm"],
        "filter": {
          "name": "mnpm",
          "kind": "lib"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'mnpm'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=mnpm",
          "--package=mnpm"
        ],
        "filter": {
          "name": "mnpm",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}
